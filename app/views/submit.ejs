<div class = "row">
	<div class = "span9">
		<form class="form-horizontal" action='/submit' method='post'>
			<fieldset>
				<legend>Submit Code!</legend>
					<div class="control-group">
						
						<label class="control-label" for="posttitle">Title: </label>
						<div class="controls">
							<input type="text" class="input-xlarge" name="posttitle" id="posttitle">
						</div>

						<br />

						<label class="control-label" for="post">Code: </label>
						<div class="controls">
							<textarea rows="10" cols="1" class="input-xlarge" name="post" id="post">
							</textarea>
						
						<br /><br />

						<button type="submit" class="btn" value="submit" >Submit</button>
						</div><br/>

						</div>
			</fieldset>
		</form>
	</div>
</div>